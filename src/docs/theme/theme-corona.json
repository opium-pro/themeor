{
  "meta": {
    "name": "corona",
    "version": "1.0",
    "contact": "Denis Elianovsky http://opium.pro",
    "description": ""
  },
  "template": ["corona"],

  "themeContext": {
    "shallInverseOn": ["base", "accent"],
    "defaultIconName": "placeholder",
    "_randomContectValue": "HERE YOU CAN SET VALUES FOR ThemeContext, BUT PLEASE START THEM WITH UNDERSCORE",
    "_formAttemtionFill": "accent"
  },

  "customVariables": {
    "random-css-variable": "HERE YOU CAN SET YOUR CSS VARIABLES",
    "demo-var": "#fc0"
  },

  "fill": {
    "base": {
      "strong": "#DBDED5",
      "weak": "#4C5A2D"
    },
    "faint": {
      "strong": "#727C59",
      "strong-down": "#6A7453",
      "weak": "#424E27",
      "weak-up": "#3A4422",
      "weak-down": "#48552B"
    },
    "accent": {
      "strong": "#FCFF5C",
      "weak": "#6F7B36",
      "weak-up": "#7B8A38"
    },
    "complement": {
      "strong": "#8889E2",
      "weak": "#EEECFD"
    },
    "critic": {
      "strong": "#FF6C6C",
      "weak": "#705E3A"
    },
    "warning": {
      "strong": "#E1922D",
      "weak": "#FCEBCF"
    },
    "success": {
      "strong": "#27AE60",
      "weak": "#DEF8E9"
    }
  },

  "fancy-fill": {
    "accent": {
      "strong": "linear-gradient(132deg, #FCFF5C, #FFDB5C)",
      "strong-down": "linear-gradient(132deg, #FCFF5C, #FFDB5C)",
      "strong-up": "linear-gradient(132deg, #FCFF5C, #FFDB5C)"
    }
  },

  "box": {
    "radius": {
      "xs": "2px",
      "sm": "4px",
      "md": "12px",
      "lg": "16px",
      "xl": "20px",
      "x2l": "40px",
      "x3l": "60px"
    },
    "shadow": {
      "md": "0px 20px 40px rgba(59, 70, 128, 0.05)",
      "lg": "0px 20px 40px rgba(6, 7, 12, 0.10)"
    }
  },

  "font": {
    "family": {
      "regular": "'Helvetica', sans-serif",
      "special": "'Montserrat', 'YOU NEED TO INCLUDE @font-face BY YOURSELF', sans-serif"
    },
    "line-height": {
      "sm": "1.2",
      "md": "1.5"
    },
    "letter-spacing": {
      "md": "0",
      "lg": "0.05em"
    },
    "size": {
      "xs": "12px",
      "sm": "14px",
      "md": "16px",
      "lg": "20px",
      "xl": "28px",
      "x2l": "40px",
      "x3l": "80px"
    }
  },

  "line": {
    "fill": {
      "faint": {
        "strong-down": "rgba(0, 0, 0, 0.15)",
        "strong": "rgba(0, 0, 0, 0.25)",
        "strong-up": "rgba(0, 0, 0, 0.35)",
        "weak": "rgba(255, 255, 255, 0.1)"
      }
    },
    "weight": {
      "md": "1px",
      "lg": "2px",
      "xl": "4px",
      "x2l": "6px"
    }
  },

  "icon": {
    "size": {
      "xs": "12px",
      "sm": "16px",
      "md": "24px",
      "lg": "32px",
      "xl": "40px"
    }
  },

  "fit": {
    "offset": {
      "x2s": "4px"
    }
  },

  "gap": {
    "x2s": "4px",
    "xs": "8px",
    "sm": "16px",
    "md": "24px",
    "lg": "32px",
    "xl": "40px",
    "x2l": "64px",
    "x3l": "80px"
  },

  "reaction": {
    "speed": {
      "md": "0.25s"
    }
  },

  "effect": {
    "transparency": {
      "x2s": "0.95",
      "xs": "0.9",
      "sm": "0.75",
      "md": "0.5",
      "lg": "0.25",
      "xl": "0.1",
      "x2l": "0.05"
    }
  }
}